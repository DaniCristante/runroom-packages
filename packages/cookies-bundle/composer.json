{
    "name": "runroom-packages/cookies-bundle",
    "type": "symfony-bundle",
    "description": "Manage cookies",
    "keywords": [
        "cookies"
    ],
    "homepage": "https://github.com/runroom/RunroomCookiesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.1.3",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\CookiesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\CookiesBundle\\Tests\\": "tests/"
        }
    }
}
